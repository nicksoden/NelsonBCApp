<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.nelsonbcapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".cafesActivity"></activity>
        <activity android:name=".DiningActivity"></activity>
        <activity android:name=".BarsActivity"></activity>
        <activity android:name=".AttractionsActivity"></activity>

    </application>

</manifest>